.lst-card{padding:0 7px}
.lst-card:after, .form-group-horizontal:after{content:"";display:block;clear:both}
.lst-card>.item{position:relative;float:left;width:50%;padding:7px}
.lst-card-wide .inner {position:relative; padding:8px;}
.lst-card-wide .card{margin-top:10px}
.lst-card-wide .img-wrap{float:left; padding:0; width:43.85%; height:auto; border-radius:6px;}
