.goods-category i:after {display:block; content:""; position:absolute; top:0; right:0; bottom:0; left:0; background:rgba(255,255,255,.8);}
.goods-category i img {width:45px;}
.goods-category .active a {color:#000; font-weight:700;}
.goods-category .active i:after {background:none;}
.goods-category li a span {margin-top:6px;display:block;}

.goods-list {}
.goods-list li {float:left; width:50%; padding:10px 10px 5px 10px; border-bottom:1px solid #ebebeb;}
.goods-list li:nth-child(odd) {padding-right:5px;}
.goods-list li:nth-child(even) {padding-left:5px;}
.goods-list li .thumb img {max-width:100%; _height:70px;}
.goods-list .name {text-align:center; padding:10px 0; font-size:12px; font-weight:700; color:#323232;}

