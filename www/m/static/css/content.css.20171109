/* ==================================================
	공통
================================================== */
.ibox{padding:10px;background:#fff}
.content .ibox{border-radius:6px}
.ibox-border{border-bottom:1px solid #ddd}
.ibox select.no-style{height:25px;padding-right:15px;background:url(../img/icon/icon_arr_down.png) no-repeat 100% 50%;background-size:7px;font-size:12px;-webkit-appearance:none;color:#808080}
.ibox select.pull-right{height:15px}
.ibox .h-title{display:inline-block;font-size:12px;line-height:1.25;vertical-align:top}
.label{display:inline-block;padding:0 .8em;background:#6fb6de;border-radius:25px;font-size:10px;line-height:1.818;vertical-align:top;text-align:center;color:#fff}
.btn-group.btn-group{padding:10px;margin-bottom:0}
.fix-btm-content .btn-group.btn-group{position:fixed;bottom:0;left:0;right:0;padding:0 10px 20px}

/* ==================================================
