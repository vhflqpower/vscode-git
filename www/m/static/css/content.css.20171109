.card-action{padding:0 10px 8px;border-top:1px solid #ececec;}
.action-box>a, .action-box>button{height:40px;padding:12px 8px;background:none;border:0;font-size:11px}
.card-action .collapse-content{margin:0 -10px;padding:0 10px;border-top:1px solid #ececec}
.collapse-content [data-toggle=collapse]{display:block;padding:1em 0;font-size:11px;text-align:center;color:#808080}
.content-more {display:block;padding:1em 0;font-size:11px;text-align:center;color:#808080}
.lst-tmb .media{border-bottom:1px solid #ececec}
.tmb-text .user{display:block;margin-bottom:5px;font-size:14px;font-weight:700;color:#323232}
.tmb-text .date{font-size:11px; color:#808080; font-weight:400; vertical-align:1px;}
.tmb-text .icon-del {height:22px;}
[class*=lst-tmbline]{border-bottom:1px solid #ebebeb}
