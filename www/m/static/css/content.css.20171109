================================================== */
.btn-goods-total {width:100%; position:relative; background:#fff; height:22px; border:none; outline:0;}
.btn-goods-total img {width:10px; vertical-align:middle;}
#top10_cateClose {background:#eeeef0;}

.category-page {padding:0 0 10px 0;}

.total-category {position:fixed; top:85px; right:0; left:0; z-index:45; background:#fff;}
.total-category li {float:left; width:33.33%; border:1px solid #eeeef0; border-width:0 1px 1px 0;}
.total-category li:nth-child(3n+3) {border-right:none;}
.total-category li a {display:block; text-align:center; color:#000; font-size:12px; font-weight:700; line-height:40px;}

.total-category.static {position:static;}
.total-category.static i {display:inline-block;}
.total-category.static i img {width:45px;}
.total-category.static a {padding:16px 0;line-height: inherit;}
.total-category.static span {display:block; margin-top:7px;}


